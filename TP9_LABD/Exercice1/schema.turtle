@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix ns: <http://www.labd.org/2015/voitures/schema#> .

ns:Voiture a rdfs:Class .

ns:nom
  a rdfs:Property ;
  rdfs:domain ns:Voiture ;
  rdfs:range rdfs:Literal .

ns:marque
  a rdfs:Property ;
  rdfs:domain ns:Voiture ;
  rdfs:range rdfs:Literal .

ns:date
  a rdfs:Property ;
  rdfs:domain ns:marque ;
  rdfs:range rdfs:Literal .
